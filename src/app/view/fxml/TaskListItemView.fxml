<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.StackPane?>

<AnchorPane fx:id="taskListItemViewLayout" maxWidth="1.7976931348623157E308"
	prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="app.view.TaskListItemViewManager">
	<children>
		<HBox alignment="CENTER_RIGHT" centerShape="false" fillHeight="false"
			maxWidth="1.7976931348623157E308" prefHeight="46.0"
			AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
			AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<children>
				<Label fx:id="taskId" alignment="CENTER" maxWidth="1.7976931348623157E308"
					minWidth="60.0" prefHeight="46.0" prefWidth="60.0" text="ID">
					<font>
						<Font size="14.0" />
					</font>
				</Label>
				<Label fx:id="taskName" maxWidth="1.7976931348623157E308"
					prefHeight="46.0" prefWidth="386.0"
					text="Task Name Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore "
					wrapText="true" HBox.hgrow="ALWAYS">
					<font>
						<Font size="14.0" />
					</font>
				</Label>
				<Separator orientation="VERTICAL" prefHeight="46.0"
					prefWidth="6.0">
					<padding>
						<Insets bottom="3.0" top="3.0" />
					</padding>
					<HBox.margin>
						<Insets left="4.0" right="4.0" />
					</HBox.margin>
				</Separator>
				<VBox alignment="CENTER_RIGHT" nodeOrientation="LEFT_TO_RIGHT">
					<children>
						<Label fx:id="taskTopDate" alignment="CENTER_RIGHT"
							minHeight="-Infinity" minWidth="-Infinity" prefHeight="23.0"
							text="To 10/11/2015 3:15pm">
							<font>
								<Font size="11.0" />
							</font>
						</Label>
						<Label fx:id="taskBottomDate" alignment="CENTER_RIGHT"
							minHeight="-Infinity" minWidth="-Infinity" prefHeight="23.0"
							text=" 3:15pm">
							<font>
								<Font size="11.0" />
							</font>
						</Label>
					</children>
				</VBox>
				<CheckBox fx:id="taskCheckbox" alignment="CENTER"
					contentDisplay="CENTER" mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT">
					<HBox.margin>
						<Insets left="10.0" right="5.0" />
					</HBox.margin>
				</CheckBox>
			</children>
		</HBox>
	</children>
</AnchorPane>
